{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/ais-proxy",
      "dest": "/api/ais-proxy.js",
      "methods": ["GET", "POST", "OPTIONS"],
      "headers": {
        "Connection": "Upgrade",
        "Upgrade": "websocket"
      }
    }
  ]
}
